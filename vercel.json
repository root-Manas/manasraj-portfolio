{
    "buildCommand": null,
    "outputDirectory": ".",
    "framework": null,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*)\\.(css|js|ico|png|jpg|jpeg|gif|svg|webm|webp|woff|woff2)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}